{
   "name": "firestations-db",
   "version": "1.0.0",
   "description": "FEMA Firestations Map Database",
   "main": "index.js",
   "scripts": {
      "setup": "npm install --prefix client && npm install --prefix server",
      "build": "npm run build --prefix client && npm run build --prefix server",
      "start": "cd server && npm start",
      "client-dev": "cd client && npm start",
      "client-build": "cd client && npm run build",
      "server-dev": "cd server && npm run dev",
      "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run setup && npm run build"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/slutske22/firestations-db.git"
   },
