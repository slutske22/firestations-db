{
   "name": "firestations-db-server",
   "version": "1.0.0",
   "description": "server side for firestations-db",
   "main": "index.js",
   "scripts": {
      "dev": "npx nodemon src/index.js --exec babel-node --presets babel-preset-env",
      "transpile": "./node_modules/.bin/babel src --out-dir build",
      "clean": "rimraf build",
      "build": "npm run clean && npm run transpile",
      "start": "npm run build && node build"
   },
   "author": "Seth Lutske",
   "license": "ISC",
   "dependencies": {
      "body-parser": "^1.20.1",
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "geocodio-library-node": "^1.3.0",
      "mongoose": "^5.9.18",
      "rimraf": "^3.0.2"
   },
   "devDependencies": {
      "babel-cli": "^6.26.0",
      "babel-core": "^6.26.3",
      "babel-loader": "^8.1.0",
      "babel-preset-env": "^1.7.0",
      "nodemon": "^2.0.4"
   }
}
